package com.cybage.service;

import java.sql.SQLException;

import com.cybage.model.Category;

public interface AdminService {

	public int addCategory(Category category) throws SQLException;
	public void updateCategory(int catId) throws SQLException;
	public void deleteCategory(int catId) throws SQLException;
	public Category getCategoryById(int catId) throws SQLException;
	public Category listCategory(Category category) throws SQLException;

}
